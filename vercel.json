{
  "version": 2,
  "rewrites": [
    { "source": "/api/collect", "destination": "/api/collect" },
    { "source": "/static/:path*", "destination": "/static/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/collect",
      "schedule": "0 11 * * *"
    }
  ]
}
